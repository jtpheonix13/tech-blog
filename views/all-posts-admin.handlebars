{{#each posts as |post|}} 
<div class="card">
    <a href="/dashboard/edit/{{post.id}}">
        <div>
            <h2>
                {{post.title}}
            </h2>
            <p>Posted by {{post.User.username}} on {{format_date post.updatedAt}}</p>
        </div>
</a>
</div>
<div class="card">
    <a href="/dashboard/new">
        <button class="btn" id="new-post">Add new post</button>
    </a>
</div>
{{/each}}